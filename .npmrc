# Configurações do npm
save-exact=true
package-lock=true
engine-strict=true

# Configurações de segurança
audit=true
fund=false

# Configurações de cache
cache-min=3600
cache-max=86400

# Configurações de registro
registry=https://registry.npmjs.org/

# Configurações de scripts
script-shell=powershell.exe
